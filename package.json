{
  "name": "my-cloudflare-docker-proxy",
  "private": true,
  "description": "Cloudflare Workers Docker Registry Proxy - 支持多个Docker镜像仓库的代理服务",
  "version": "1.0.0",
  "author": "ciiiii <allen.cccccnm@gmail.com>",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=4.0.0"
  },
  "packageManager": "yarn@4.9.1",
  "dependencies": {},
  "devDependencies": {
    "prettier": "^2.4.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "wrangler": "^3.114.9"
  },
  "scripts": {
    "format": "prettier --write '**/*.{js,css,json,md}'",
    "build": "webpack",
    "build:worker": "webpack --config webpack.config.js",
    "verify": "node build.js",
    "verify:worker": "node build-worker.js",
    "dev": "npx wrangler dev src/index.js --env dev",
    "deploy": "npx wrangler deploy src/index.js --env production",
    "deploy:staging": "npx wrangler deploy src/index.js --env staging",
    "deploy:direct": "npx wrangler deploy src/index.js --env production",
    "clean": "rm -rf dist .yarn/cache"
  },
  "license": "MIT",
  "main": "src/index.js"
}
